{
  "name": "Jump To JumpTube",
  "description": "View this video as jumptube",
  "version": "1.2",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
	"tabs",
	"https://www.youtube.com/*"
  ],
  "browser_action": {
      "default_title": "View In JumpTube",
	  "default_popup": "hello.html",
      "default_icon": "jumpTube_16x16.png"
  },
  "manifest_version": 2
}
